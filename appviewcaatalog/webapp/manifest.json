{
    "_version": "1.49.0",
    "sap.app": {
        "id": "appviewcaatalog",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.9.0",
            "toolsId": "3257583f-9a55-480c-8bee-3dbe6490e437"
        },
        "crossNavigation": {
            "inbounds": {
                "appviewcaatalog-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "appviewcatalog",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        },
        "dataSources": {
            "products-srv" : {
                "uri": "localservice/db.data.json",
                "type": "JSON"
            },
            "tiles-srv" : {
                "uri": "localservice/tiles.config.json",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.111.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "appviewcaatalog.i18n.i18n"
                }
            },
            "mProduct" : {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "products-srv"
            },
            "mTile" : {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "tiles-srv"
            }
            
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "appviewcaatalog.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteVwMain",
                    "pattern": ":?query:",
                    "target": [
                        "TargetVwMain"
                    ]
                },
                {
                    "name": "ViewList",
                    "pattern": "patternViewList",
                    "target": [
                        "TargetList"
                    ]
                },
                {
                    "name": "ViewForm",
                    "pattern": "patternViewForm",
                    "target": [
                        "TargetForm"
                    ]
                }                
            ],
            "targets": {
                "TargetVwMain": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "VwMain",
                    "viewName": "VwMain"
                },
                "TargetList": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "List",
                    "viewName": "List"
                },
                "TargetForm": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Form",
                    "viewName": "Form"
                }                
            }
        },
        "rootView": {
            "viewName": "appviewcaatalog.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "appviewcaatalog"
    }
}